	"name": "locx-re-web",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"preview": "vite preview",
		"build": "tsc && vite build",
		"build:staging": "tsc && vite build --mode staging",
		"test": "jest --passWithNoTests",
		"test:watch": "jest --passWithNoTests --watch",
		"ci:e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
		"ci:e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
		"dev": "vite",
		"start": "npm run dev",
		"analyze": "react-app-rewired build && source-map-explorer 'build/static/js/*.js' --html analyze-result.html",
		"lint": "eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint ./src --ext ts,tsx --fix",
		"prettier": "npx prettier ./src --check",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix",
		"audit": "npm audit --production",
		"commitlint": "commitlint --edit",
		"prepare": "husky && husky install"
	},
